{
  "name": "oakadmin",
  "description": "A CLI tool to generate MERN admin panel for your project.",
  "repository": "https://github.com/oaktreeapps/admin_panel_cli",
  "homepage": "https://oaktreeapps.com",
  "author": {
    "email": "hello@oaktreeapps.com",
    "name": "OakTree Apps",
    "url": "https://oaktreeapps.com"
  },
  "version": "0.5.7",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "bin": "dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "tsup src/index.ts --format esm --minify --treeshake recommended",
    "watch": "yarn build --watch",
    "prettify": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "lint": "eslint src/",
    "prepare": "husky install"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "commander": "^10.0.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.1.5",
    "ora": "^6.3.0",
    "simple-git": "^3.17.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.15.13",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "prettier": "2.8.8",
    "react": "^18.2.0",
    "tsup": "^6.7.0",
    "typescript": "5.0",
    "husky": "^8.0.0"
  }
}
